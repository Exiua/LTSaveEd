                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Spacing="10">
                                    <TextBlock Text="Subspecies Override:" VerticalAlignment="Center" />
                                    <ComboBox Width="150" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Spacing="10">
                                    <TextBlock Text="Takes After Mother:" VerticalAlignment="Center" />
                                    <CheckBox VerticalAlignment="Center" />
                                </StackPanel>
                            </StackPanel>
                        </TabItem>
                        <TabItem>
                            <TabItem.Header>
                                <TextBlock Text="Head" VerticalAlignment="Center" />
