alonia<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:LTSaveEd.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="1920" d:DesignHeight="1080"
        x:Class="LTSaveEd.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="LTSaveEd">

    <Design.DataContext>
        <!-- This only sets the DataContext for the previewer in an IDE,
             to set the actual DataContext for runtime, set the DataContext property in code (look at App.axaml.cs) -->
        <vm:MainWindowViewModel />
    </Design.DataContext>

    <Panel>
        <DockPanel>
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="_Open">
                    <MenuItem Header="_Import..." Command="{Binding LoadFile}" />
                </MenuItem>
                <MenuItem Header="_Save As">
                    <MenuItem Header="Overwrite" Command="{Binding OverwriteFile}" />
                    <MenuItem Header="Export..." Command="{Binding SaveFile}" />
                </MenuItem>
                <MenuItem Header="Update?" />
            </Menu>

            <TabControl>
                <TabItem>
                    <TabItem.Header>
                        <TextBlock Text="Core" VerticalAlignment="Center" />
                    </TabItem.Header>
                    <StackPanel Orientation="Horizontal" Spacing="500" Margin="20">
                        <StackPanel Orientation="Vertical" Spacing="10">
                            <StackPanel Orientation="Horizontal" Spacing="10" HorizontalAlignment="Center">
                                <TextBlock Text="Character:" VerticalAlignment="Center" />
                                <AutoCompleteBox Width="175" />
                                <Button Width="175" Height="30" Foreground="Red" HorizontalContentAlignment="Center"
                                        VerticalContentAlignment="Center">
                                    DELETE CHARACTER
                                </Button>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Id:" VerticalAlignment="Center" />
                                <TextBox Text="{Binding CoreTabViewModel.CharacterId}" Width="250" IsReadOnly="True" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Name (A, F, M):" VerticalAlignment="Center" />
                                <TextBox Text="{Binding CoreTabViewModel.AndrogynousName}" Width="150" />
                                <TextBox Text="{Binding CoreTabViewModel.FeminineName}" Width="150" />
                                <TextBox Text="{Binding CoreTabViewModel.MasculineName}" Width="150" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Description:" VerticalAlignment="Center" />
                                <TextBox Text="{Binding CoreTabViewModel.Description}" Width="475" Height="75"
                                         AcceptsReturn="True" TextWrapping="Wrap" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Level:" VerticalAlignment="Center" />
                                <NumericUpDown Text="{Binding CoreTabViewModel.Level}" ShowButtonSpinner="False"
                                               Width="150" />
                                <TextBlock Text="Experience:" VerticalAlignment="Center" />
                                <NumericUpDown Text="{Binding CoreTabViewModel.Experience}" ShowButtonSpinner="False"
                                               Width="150" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Money:" VerticalAlignment="Center" />
                                <NumericUpDown Text="{Binding CoreTabViewModel.Money}" ShowButtonSpinner="False"
                                               Width="150" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Date of Birth:" VerticalAlignment="Center" />
                                <NumericUpDown Text="{Binding CoreTabViewModel.Birthday.Year}"
                                               ShowButtonSpinner="False" Width="150" />
                                <ComboBox Width="150" SelectedIndex="0" />
                                <NumericUpDown Text="{Binding CoreTabViewModel.Birthday.Day}" ShowButtonSpinner="False"
                                               Width="150" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Job History:" VerticalAlignment="Center" />
                                <ComboBox Width="150" SelectedIndex="0" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Orientation:" VerticalAlignment="Center" />
                                <ComboBox Width="150" SelectedIndex="0" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Obedience:" VerticalAlignment="Center" />
                                <NumericUpDown Text="{Binding CoreTabViewModel.Obedience}" ShowButtonSpinner="False"
                                               Width="150" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Gender Identity:" VerticalAlignment="Center" />
                                <ComboBox Width="200" SelectedIndex="0" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Perk Points:" VerticalAlignment="Center" />
                                <NumericUpDown Text="{Binding CoreTabViewModel.PerkPoints}" ShowButtonSpinner="False"
                                               Width="150" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Essence Count:" VerticalAlignment="Center" />
                                <NumericUpDown Text="{Binding CoreTabViewModel.EssenceCount}" ShowButtonSpinner="False"
                                               Width="150" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Health:" VerticalAlignment="Center" />
                                <NumericUpDown Text="{Binding CoreTabViewModel.Health}" ShowButtonSpinner="False"
                                               Width="150" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Mana:" VerticalAlignment="Center" />
                                <NumericUpDown Text="{Binding CoreTabViewModel.Mana}" ShowButtonSpinner="False"
                                               Width="150" />
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Orientation="Vertical" Spacing="10" Height="550">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Height="50">
                                <TextBlock Text="Personality" TextAlignment="Center" FontWeight="Bold" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="20" Height="50">
                                <CheckBox Command="{Binding CoreTabViewModel.ToggleConfidentTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Confident}">
                                    Confident
                                </CheckBox>
                                <CheckBox Command="{Binding CoreTabViewModel.ToggleShyTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Shy}">
                                    Shy
                                </CheckBox>
                                <CheckBox Command="{Binding CoreTabViewModel.ToggleKindTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Kind}">
                                    Kind
                                </CheckBox>
                                <CheckBox Command="{Binding CoreTabViewModel.ToggleSelfishTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Selfish}">
                                    Selfish
                                </CheckBox>
                                <CheckBox Command="{Binding CoreTabViewModel.ToggleNaiveTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Naive}">
                                    Naive
                                </CheckBox>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="20" Height="50">
                                <CheckBox Command="{Binding CoreTabViewModel.ToggleCynicalTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Cynical}">
                                    Cynical
                                </CheckBox>
                                <CheckBox Command="{Binding CoreTabViewModel.ToggleBraveTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Brave}">
                                    Brave
                                </CheckBox>
                                <CheckBox Command="{Binding CoreTabViewModel.ToggleCowardlyTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Cowardly}">
                                    Cowardly
                                </CheckBox>
                                <CheckBox Command="{Binding CoreTabViewModel.ToggleLewdTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Lewd}">
                                    Lewd
                                </CheckBox>
                                <CheckBox Command="{Binding CoreTabViewModel.ToggleInnocentTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Innocent}">
                                    Innocent
                                </CheckBox>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="20" Height="50">
                                <CheckBox Command="{Binding CoreTabViewModel.TogglePrudeTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Prude}">
                                    Prude
                                </CheckBox>
                                <CheckBox Command="{Binding CoreTabViewModel.ToggleLispTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Lisp}">
                                    Lisp
                                </CheckBox>
                                <CheckBox Command="{Binding CoreTabViewModel.ToggleStutterTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Stutter}">
                                    Stutter
                                </CheckBox>
                                <CheckBox Command="{Binding CoreTabViewModel.ToggleMuteTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Mute}">
                                    Mute
                                </CheckBox>
                                <CheckBox Command="{Binding CoreTabViewModel.ToggleSlovenlyTrait}"
                                          IsChecked="{Binding CoreTabViewModel.Slovenly}">
                                    Slovenly
                                </CheckBox>
                            </StackPanel>
                        </StackPanel>
                    </StackPanel>
                </TabItem>
                <TabItem>
                    <TabItem.Header>
                        <TextBlock Text="Attributes" VerticalAlignment="Center" />
                    </TabItem.Header>
                    <Grid ColumnDefinitions="750, Auto" RowDefinitions="Auto, Auto" HorizontalAlignment="Center"
                          VerticalAlignment="Center">
                        <StackPanel Orientation="Vertical" Spacing="10" Grid.Row="0" Grid.Column="0">
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Health Core:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Mana Core:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Experience:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Action Points:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Arousal:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Lust Core:" VerticalAlignment="Center" />
