                                <TextBox Width="150" />
                                <TextBlock Text="/ 80" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Critical Damage:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 500" VerticalAlignment="Center" />
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Orientation="Vertical" Spacing="10" Grid.Row="0" Grid.Column="1">
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Energy Shielding:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 500" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Physical Resistance:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 500" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Lust Resistance:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 500" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Fire Resistance:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 500" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Ice Resistance:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 500" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Poison Resistance:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 500" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Unarmed Damage:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 100" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Melee Damage:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 100" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Ranged Damage:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 100" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Spell Damage:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 100" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Physical Damage:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 100" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Lust Damage:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 100" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Fire Damage:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 100" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Ice Damage:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 100" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Poison Damage:" VerticalAlignment="Center" />
                                <TextBox Width="150" />
                                <TextBlock Text="/ 100" VerticalAlignment="Center" />
                            </StackPanel>
                        </StackPanel>
                    </Grid>
                </TabItem>
                <TabItem>
                    <TabItem.Header>
                        <TextBlock Text="Body" VerticalAlignment="Center" />
                    </TabItem.Header>
                    <TabControl>
                        <TabItem>
                            <TabItem.Header>
                                <TextBlock Text="Body Core" VerticalAlignment="Center" />
                            </TabItem.Header>
                            <StackPanel Orientation="Vertical" Spacing="10" Margin="20">
                                <StackPanel Orientation="Horizontal" Spacing="10">
                                    <TextBlock Text="Body Material:" VerticalAlignment="Center" />
                                    <ComboBox Width="150" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Spacing="10">
                                    <TextBlock Text="Body Size:" VerticalAlignment="Center" />
                                    <TextBox Width="150" />
                                    <TextBlock Text="Label" VerticalAlignment="Center" />
