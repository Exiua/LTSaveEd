                                <TextBlock Text="/ 100" VerticalAlignment="Center" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="10">
                                <TextBlock Text="Resting Lust:" VerticalAlignment="Center" />
